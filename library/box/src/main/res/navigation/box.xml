<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/box_list_fragment">
    <fragment
        android:id="@+id/box_list_fragment"
        android:name="com.sorrowblue.comicviewer.library.box.list.BoxListFragment"
        android:label="BoxListFragment"
        tools:layout="@layout/library_fragment_cloud">
        <action
            android:id="@+id/action_box_list_to_box_signin"
            app:destination="@id/box_signin_fragment"
            app:popUpTo="@id/box_list_fragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="parent"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="transition_name"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_box_list_self"
            app:destination="@id/box_list_fragment" />
    </fragment>
    <fragment
        android:id="@+id/box_signin_fragment"
        android:name="com.sorrowblue.comicviewer.library.box.signin.BoxSignInFragment"
        android:label="Box"
        tools:layout="@layout/googledrive_fragment_signin" />
</navigation>
